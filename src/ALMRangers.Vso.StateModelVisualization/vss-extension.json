{
    "manifestVersion": 1,
    "id": "almrangers-vsoextensions-StateModelVisualization",
    "version": "1.0.0",
    "name": "State Model Visualization",  
    "description": "An extension for Visual Studio Online that adds the ability to view the state model for a selected work item.",
  "publisher": "alm-rangers-taavik",
  "scopes": [ "vso.work" ],
    "baseUri": "https://almrangers-vsoextensions-statemodelvisualization.azurewebsites.net/",
    "icons": {
      "wide": "images/statemodel_wide_icon.png",
      "default": "images/statemodel_short_icon.png"
    },
    "categories": [
      "Productivity"
    ],
     "contributions": [
         {
             "id": "state-model-visualization-hub",
             "type": "ms.vss-web.hub",
             "description": "Adds a 'State Model Visualization' hub to the Work hub group.",
             "targets": [
                 "ms.vss-work-web.work-hub-group"
             ],
             "properties": {
                 "name": "State Visualize",
                 "order": 99,
                 "uri": "app.html"
             }
         }
     ]
}
