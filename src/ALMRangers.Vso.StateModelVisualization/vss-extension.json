{
  "manifestVersion": 1,
  "id": "tk-vsoextensions-StateModelVisualization",
  "version": "1.1.0",
  "name": "State Model Visualization",
  "description": "Add the ability to view the state model for a selected work item type.",
  "publisher": "taavi-koosaar",
  "public": true,
  "scopes": [ "vso.work" ],
  "icons": {
    "default": "images/StateModelShortIcon.png"
  },
  "categories": [
    "Developer samples"
  ],
  "tags": [
    "work item types",
    "visualize visualization"
  ],
  "screenshots": [
    {
      "path": "screenshots/screen1.png"
    },
    {
      "path": "screenshots/screen2.png"
    }
  ],
  "links": {
    "home": {
      "uri": "http://aka.ms/vsoext11"
    },
    "support": {
      "uri": "http://aka.ms/vsarcontact"
    },
    "issues": {
      "uri": "http://aka.ms/vsoext11"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "contributions": [
    {
      "id": "state-model-visualization-hub",
      "type": "ms.vss-web.hub",
      "description": "Adds a 'State Model Visualization' hub to the Work hub group.",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "State Visualizer",
        "order": 99,
        "uri": "app.html"
      }
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "License.txt"
    }
  },
  "files": [
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "sdk",
      "addressable": true
    },
    {
      "path": "css",
      "addressable": true
    },
    {
      "path": "app.html",
      "addressable": true
    },
        {
      "path": "ThirdPartyNotice.txt",
      "addressable": true
    }
  ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ]
}
