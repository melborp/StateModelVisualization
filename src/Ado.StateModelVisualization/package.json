{
  "name": "ado-state-model-visualization",
  "version": "2.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "webpack serve --mode development",
    "build": "npm run clean && webpack --mode production",
    "build:dev": "npm run clean && webpack --mode development",
    "package": "npm run build && tfx extension create --manifests vss-extension.json --rev-version --output-path out/",
    "devpack": "npm run build:dev && tfx extension create --manifests vss-extension.json --overrides-file vss-extension.dev.internal.json --rev-version --output-path out/",
    "intpack": "npm run build && tfx extension create --manifests vss-extension.json --overrides-file vss-extension.internal.json --rev-version --output-path out/"
  },
  "dependencies": {
    "azure-devops-extension-api": "^4.259.0",
    "azure-devops-extension-sdk": "^4.0.2",
    "azure-devops-ui": "^2.259.0",
    "cytoscape": "^3.26.0",
    "cytoscape-dagre": "^2.5.0",
    "dagre": "^0.8.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@types/cytoscape": "^3.19.0",
    "@types/dagre": "^0.7.53",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "tfx-cli": "^0.22.1",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  }
}
